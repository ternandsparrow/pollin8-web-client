{
  "name": "pollin8-web-client",
  "version": "1.0.0",
  "description": "Web client for the Pollin8 simulation engine",
  "author": "TERN and Sparrow",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "test": "ava",
    "test:watch": "ava --watch",
    "generate": "nuxt generate",
    "lint": "eslint './**/*.{js,vue}'",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@mdi/font": "^5.0.45",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/sentry": "^4.5.0",
    "@turf/area": "^6.0.1",
    "@turf/boolean-contains": "^6.0.1",
    "@turf/transform-scale": "^5.1.5",
    "@turf/transform-translate": "^5.1.5",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "chart.js": "^2.8.0",
    "cross-env": "^7.0.2",
    "d3": "5.16.0",
    "eslint-plugin-vue": "6.2.2",
    "leaflet-draw": "^1.0.4",
    "leaflet-sleep": "https://github.com/ternandsparrow/Leaflet.Sleep",
    "moment": "^2.29.2",
    "nuxt": "^2.14.7",
    "nuxt-env": "^0.1.0",
    "nuxt-leaflet": "^0.0.21",
    "socket.io-client": "^4.0.1",
    "vue-chartjs": "^3.4.2",
    "vue-izitoast": "^1.1.2",
    "vue-pdf": "^4.0.8",
    "vue-scrollto": "^2.15.0",
    "vue-vimeo-player": "^0.1.0",
    "vuetify": "^1.5.24",
    "vuetify-loader": "^1.2.0"
  },
  "devDependencies": {
    "@babel/register": "^7.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.4.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.2.6",
    "eslint": "^7.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/setup.js"
    ],
    "files": [
      "./test/specs/*.js"
    ],
    "snapshotDir": "./test/snapshots"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "@": "./",
            "~": "./"
          }
        }
      ]
    ]
  }
}
